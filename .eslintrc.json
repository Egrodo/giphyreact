{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "react/no-did-mount-set-state": 0,
        "react/forbid-prop-types": 0,
        "linebreak-style": 0,
        "no-console": 0
    }
}